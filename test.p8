function init_caves()
	cave={}
	cave.exitx=256
	cave.exity=0
end

function update_caves()
	if	map_collide(pl.x,pl.y,
		pl.w,pl.h,4)==true then
		cx=cave.exitx
		cy=cave.exity
		pl.x=cx+8
		pl.y=cy+16
		sfx(5)
	end
end
